<h2>All events</h2>
<div class="allEvents">
    {{#each event}}
  {{!-- <div class="container"> --}}
    <article id="list">
      <p>{{this.name}}</p>
      <p>{{this.description}}</p>
      <p>{{this.location}}</p>
      <p>{{this.activity}}</p>
      <p><img href="{{this.image}}" /></p>
    </article>
  {{!-- </div> --}}
  {{/each}}
</div>
