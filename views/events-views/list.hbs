<h2>All events</h2>
<div class="allEvents">
  {{#each event}}
    {{!-- <div class="container"> --}}
    <article id="list">
      <p>{{this.name}}</p>
      <p>{{this.description}}</p>
      <p>{{this.location}}</p>
      <p>{{this.activity}}</p>
      <p>{{this.email}}</p>
      <p><img href="{{this.image}}" /></p>

        <a href="/events/delete/{{this._id}}"><p>Delete</p></a>
        
    </article>

    {{!-- </div> --}}
  {{/each}}
</div>

